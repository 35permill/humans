<launch>
 <node name="lr_arm"
    pkg="pr2_controller_manager" type="pr2_controller_manager"
    args="stop l_arm_controller" />

 <rosparam ns="l_cart">
   type: robot_mechanism_controllers/CartesianTwistController
   root_name: torso_lift_link
   tip_name: l_gripper_tool_frame
   fb_trans:
     p: 20.0
     i: 0.5
     d: 0.0
     i_clamp: 1.0
   fb_rot:
     p: 0.5
     i: 0.1
     d: 0.0
     i_clamp: 0.2
 </rosparam>
<node name="spawn_cart"
       pkg="pr2_controller_manager" type="spawner"
       args="l_cart" />
</launch>
